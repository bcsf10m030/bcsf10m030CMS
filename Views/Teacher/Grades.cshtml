@model List <project.Models.enrolledStudent>

        <form method="post" action="~/Teacher/Grades" >
         <table>
               <tr>
                   <td><h3> RollNumber </h3></td>
                    <td> <h3> Name     </h3></td>
                    <td><h3>  Sessional     </h3></td>
                    <td><h3>  Mids       </h3></td>
                    <td><h3>  Final      </h3></td>

               </tr>
             
            @if(Model.Count>0)
            {
            for( int i=0 ; i<Model.Count ; i++){
                <tr>
                      <td>    @Html.DisplayFor(model=>model[i].userName )   </td>
                      <td>    @Html.DisplayFor(model=>model[i].users1.name )       </td>
                      <td>    @Model[i].sessional</td>
                      <td>    @Model[i].mid   </td>
                      <td>    @Model[i].final    </td>
                      <td>    @Html.ActionLink("Edit", "Edit", new { n = Model[i].userName , c=Model[i].courseName , t=Model[i].tName })  </td>                                                                      
                      

                </tr>
            }
            }
           </table> 
          
           </form>


